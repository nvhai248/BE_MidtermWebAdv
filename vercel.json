{
    "version": 2,
    "builds": [
        {
            "src": "src/index.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "src/index.js"
        },
        {
            "src": "/(.*)",
            "dest": "src/index.js",
            "methods": [
                "GET",
                "POST",
                "DELETE",
                "PATCH"
            ],
            "headers": [
                "Origin",
                "Authorization",
                "Content-Type"
            ],
            "allowCredentials": true,
            "maxAge": 3600
        }
    ]
}